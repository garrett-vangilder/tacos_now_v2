{
  "name": "tacos-now-server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "assets": "webpack -w --display-max-modules 0",
    "develop": "NODE_ENV=test concurrently \"npm run development-msg\" \"npm run watch\" \"npm run assets\"",
    "development-msg": "echo 'Running in development mode...' && exit 0",
    "lint": "eslint ./ --cache --ignore-pattern .gitignore",
    "precommit-msg": "echo 'Pre-commit checks...' && exit 0",
    "start": "concurrently \"npm run watch\" \"npm run assets\"",
    "test": "NODE_ENV=test nodemon --exec 'mocha --recursive -R min'",
    "watch": "nodemon ./start.js --ignore public/"
  },
  "pre-commit": [
    "precommit-msg",
    "lint"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "0.15.3",
    "body-parser": "1.17.1",
    "bulma": "^0.5.1",
    "chai": "^4.1.1",
    "chalk": "^2.1.0",
    "connect-mongo": "1.3.2",
    "dotenv": "4.0.0",
    "express": "4.15.2",
    "express-session": "1.15.1",
    "express-validator": "3.1.2",
    "forever": "0.15.3",
    "glob": "7.1.1",
    "md5": "2.2.1",
    "mocha": "^3.5.0",
    "moment": "2.17.1",
    "mongoose": "4.8.7",
    "mongoose-mongodb-errors": "0.0.2",
    "pug": "2.0.0-beta6",
    "supertest": "^3.0.0",
    "uuid": "3.0.1",
    "validator": "7.0.0"
  },
  "devDependencies": {
    "autoprefixer": "6.7.7",
    "babel-core": "6.24.0",
    "babel-loader": "6.4.0",
    "babel-preset-es2015": "6.24.0",
    "concurrently": "3.4.0",
    "css-loader": "0.27.3",
    "eslint": "^4.3.0",
    "eslint-config-airbnb-base": "^11.3.1",
    "eslint-plugin-import": "^2.7.0",
    "extract-text-webpack-plugin": "2.1.0",
    "node-sass": "^4.5.3",
    "nodemon": "1.11.0",
    "postcss-loader": "1.3.3",
    "pre-commit": "^1.2.2",
    "sass-loader": "^6.0.3",
    "webpack": "^2.2.1"
  }
